	<WidgetLook name="FOPCSimLook/Combobox">
		<Child nameSuffix="__auto_editbox__" type="FOPCSimLook/ComboEditbox">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0"/>
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0"/>
				</Dim>
				<Dim type="RightEdge">
