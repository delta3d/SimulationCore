								<DimOperator op="Multiply">
									<AbsoluteDim value="0.75"/>
								</DimOperator>
							</FontDim>
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="Height">
					<FontDim type="LineSpacing">
						<DimOperator op="Multiply">
							<AbsoluteDim value="1.5"/>
						</DimOperator>
					</FontDim>
				</Dim>
			</Area>
		</Child>
		<Child nameSuffix="__auto_droplist__" type="FOPCSimLook/ComboDropList">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0"/>
				</Dim>
