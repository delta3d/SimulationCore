         <source type="Fragment">Shaders/Base/perpixel_lighting_fragment_functions.glsl</source>
         <parameter name="diffuseTexture">
            <texture2D textureUnit="0">
               <source type="Auto"/>
            </texture2D>
         </parameter>
         <parameter name="detailTexture">
            <texture2D textureUnit="1">
               <source type="Image">Textures/ShadersBase/entity_detailmap.png</source>
               <wrap axis="S" mode="Repeat"/>
               <wrap axis="T" mode="Repeat"/>
